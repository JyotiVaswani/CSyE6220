<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
    <class name = "com.CSyE6220.TechWorldHustle.pojo.Candidate" table = "Candidate">
        <id name="userId" column= "CandidateId" type="int">
            <generator class="native"/>
        </id>
        
        <property name="firstName" column="FirstName" type="string"/>
        <property name="lastName" column="LastName" type="string"/>
        <property name="current_Company" column="Current_Company" type="string"/>
        <property name="dob" column="DOB" type="date"/>
        <property name="gender" column="gender" type="string"/>
        <property name="contact" column="Contact" type="string"/>
        <property name="current_location" column="Current_location" type="string"/>
        <property name="current_JobTitle" column="Current_JobTitle" type="string"/>
        <property name="cv" column="CV" type="string"/>
        <property name="userName" column="Username" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="primary_skills" column="keyskills" type="string"/>
        <property name="employmentTypePreference" column="employmentTypePreference" type="string"/>
                       
    </class>          
</hibernate-mapping>